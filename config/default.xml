<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<entry key="INSTANCE_FOLDER">./instances/</entry>
<entry key="RESULT_FOLDER">./results/</entry> 

<entry key="PRINT_IN_CONSOLE">false</entry>
<entry key="PRINT_POOLS_TO_FILE">false</entry>

<entry key="PRECISION">4</entry> 
<entry key="SEED">1</entry> 
<entry key="THREADS">10</entry> 

<entry key="SPLIT_IMPROVE_PETAL_LKH">true</entry>
<entry key="SPLIT_ADD_ALL">false</entry> 

<entry key="MSH_MAX_POOL_SIZE">20000</entry> 
<entry key="MSH_NUM_ITERATIONS">10000</entry>
<entry key="MSH_SAMPLING_TIME_LIMIT">60</entry>
<entry key="MSH_RANDOM_FACTOR_HIGH">7</entry>
<entry key="MSH_RANDOM_FACTOR_LOW">3</entry>
<entry key="MSH_ASSEMBLY_TIME_LIMIT">60</entry>

</properties>
