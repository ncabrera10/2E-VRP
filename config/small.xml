<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<entry key="INSTANCE_FOLDER">./instances/</entry>
<entry key="RESULT_FOLDER">./results/test/</entry> 

<entry key="PRINT_IN_CONSOLE">false</entry>
<entry key="PRINT_POOLS_TO_FILE">false</entry>

<entry key="PRECISION">4</entry> 
<entry key="SEED">1</entry> 
<entry key="THREADS">8</entry> 


<entry key="USE_TSP_HEURISTICS_LOW">true</entry>
<entry key="USE_TSP_HEURISTICS_HIGH">true</entry>
<entry key="USE_CLUSTER_FIRST_HEURISTIC">true</entry>
<entry key="USE_TSP_WITH_CAPACITY">false</entry>
<entry key="TSP_DEPOT_STARTING_POINT">false</entry>
<entry key="TSP_CENTROID_STARTING_POINT">true</entry>

<entry key="SPLIT_IMPROVE_PETAL_LKH">true</entry>
<entry key="SPLIT_ADD_ALL">false</entry> 
<entry key="SPLIT_DUPLICATE_SATELLITE_ROUTES">false</entry>
<entry key="SPLIT_TRY_CAPACITIES">false</entry> 

<entry key="EMPHASIZE_FEASIBILITY">true</entry>

<entry key="MSH_MAX_POOL_SIZE">20000</entry> 
<entry key="MSH_NUM_ITERATIONS">5000</entry>
<entry key="MSH_SAMPLING_TIME_LIMIT">120</entry>
<entry key="MSH_RANDOM_FACTOR_HIGH">12</entry>
<entry key="MSH_RANDOM_FACTOR_HIGH_RN">5</entry>
<entry key="MSH_RANDOM_FACTOR_LOW">3</entry>
<entry key="MSH_ASSEMBLY_TIME_LIMIT">120</entry>

</properties>
